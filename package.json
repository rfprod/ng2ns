	"scripts": {
		"prestart": "npm install && npm run clear-ts-build",
		"start": "gulp",
		"production-start": "gulp production-start",
		"pretest": "npm install",
		"test": "echo 'WARNING: npm test is not configured yet\nUse one of these depending on what do you want to test:\nnpm run client-test\nnpm run server-test' && exit 1",
		"preclient-test": "mkdir -p ./logs/unit",
		"client-test": "npm run clear-reports && gulp client-unit-test-single-run && npm run client-coverage-report",
		"client-coverage-report": "remap-istanbul -i ./logs/coverage/coverage-final.json -o ./logs/coverage/html-report -t html && echo '# coverage html-report generated, path: ./logs/coverage/html-report'",
		"clear-reports": "rm -rf ./logs/coverage ./logs/unit && echo '# coverage and unit reports cleared'",
		"clear-logs": "find ./logs/ -type f -name '*[^.gitkeep]*' -delete && find ./logs/ -type d -empty -name '*[^.gitkeep]*' -delete",
		"clear-ts-build": "find ./public/app/ ./test/client/ ./public/js/ -type f -name \"*.js\" -exec rm {} \\; && npm run clear-js-maps",
		"server-test": "gulp server-test",
		"lint": "gulp watch-and-lint",
		"lint-single-run": "gulp lint",
		"doc": "npm run clear-doc && npm run logs-index && gulp jsdoc-server && gulp typedoc-client",
		"doc-docker": "npm run logs-index-docker && gulp jsdoc-server && gulp typedoc-client",
		"logs-index": "gulp generate-logs-index && bash git-stats.sh",
		"logs-index-docker": "gulp generate-logs-index && echo '<html><body><pre>no git-stats data</pre></body></html>' > ./logs/git-stats.html",
		"clear-doc": "rm -rf ./logs/jsdoc ./logs/typedoc && echo '# doc cleared'",
		"clear-js-maps": "find ./public/app/ ./test/client/ -type f -regex \".*\\.js\\..*\" -exec rm {} \\;",
		"docker": "bash ci/docker.sh",
		"docker-build": "bash ci/docker.sh build",
		"docker-start": "bash ci/docker.sh start",
		"preupdate-webdriver": "npm install",
		"update-webdriver": "webdriver-manager update",
		"clean-webdriver": "webdriver-manager clean",
		"preprotractor": "npm run update-webdriver",
		"protractor": "protractor test/protractor.conf.js",
		"electron": "electron .",
		"electron-clear-dist": "rm -rf ./desktop",
		"electron-build-win": "gulp build-electron-win",
		"electron-build-nix": "gulp build-electron-deb",
		"electron-build-osx": "gulp build-electron-osx",
		"tsc": "tsc",
		"tsc:w": "tsc -w"
	}
}
