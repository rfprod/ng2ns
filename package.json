	"scripts": {
		"prestart": "npm install && npm run clear-ts-build",
		"start": "bash npm-start.sh",
		"development-start": "tsc && concurrently \"gulp\" \"npm run tsc:w\"",
		"production-start": "gulp production-start",
		"pretest": "npm install",
		"test": "echo 'WARNING: npm test is not configured yet\nUse one of these depending on what do you want to test:\nnpm run client-test\nnpm run client-test-single-run\nnpm run server-test' && exit 1",
		"client-test": "gulp watch-client-and-test",
		"client-test-single-run": "npm run clear-reports && gulp client-unit-test-single-run && npm run client-coverage-report",
		"client-coverage-report": "remap-istanbul -i ./logs/coverage/coverage-final.json -o ./logs/coverage/html-report -t html && echo '# coverage html-report generated, path: ./logs/coverage/html-report'",
		"clear-reports": "rm -rf ./logs/coverage ./logs/unit && echo '# coverage and unit reports cleared'",
		"clear-logs": "find ./logs/ -type f -name '*[^.gitkeep]*' -delete && find ./logs/ -type d -empty -name '*[^.gitkeep]*' -delete",
		"clear-ts-build": "find ./public/app/ ./test/client/ -type f | grep \".js\" | xargs rm",
		"server-test": "gulp server-test",
		"lint": "gulp watch-and-lint",
		"lint-single-run": "gulp lint",
		"preupdate-webdriver": "npm install",
		"update-webdriver": "webdriver-manager update",
		"preprotractor": "npm run update-webdriver",
		"protractor": "protractor test/protractor.conf.js",
		"electron": "electron .",
		"electron-clear-dist": "rm -rf ./desktop",
		"tsc": "tsc",
		"tsc:w": "tsc -w"
	}
}
